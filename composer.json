{
    "name": "woocommerce/standalone-ecommerce",
    "description": "Standalone Ecommerce Application - Migrated from WooCommerce Plugin",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "slim/twig-view": "^3.3",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.5",
        "monolog/monolog": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2"
        }
    }
}
